{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "printWidth": 120,
    "bracketSameLine": false,
    "vueIndentScriptAndStyle": true,
    "htmlWhitespaceSensitivity": "strict",
    "proseWrap": "always"
}
